   "source": [
    "from tensorflow import Variable, GradientTape\n",
    "\n",
    "x0 = Variable(3.0)\n",
    "x1 = Variable(0.0)\n",
    "\n",
    "with GradientTape() as tape:\n",
    "    x1.assign_add(x0)\n",
    "    y = x1 ** 2\n",
    "dy_dx = tape.gradient(y, [x0, x1])\n",
    "dy_dx"
   ]
  }
